int main()
{
    int a[10], i, menor;

    for(i=0; i<10; i++){
        printf("digite os numeros do vetor A:");
        scanf("%d", &a[i]);
    }

    menor = a[0];
    for(i=0; i<10; i++){
        if(a[i]<menor){
            menor = a[i];
    }
}
        printf("menor numero: %d", a[i]);

    return 0;
}
